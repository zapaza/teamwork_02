{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --cache --cache-location ./node_modules/.cache/eslint",
    "lint:stylelint": "stylelint \"src/**/*.{css,pcss}\" --cache --cache-location ./node_modules/.cache/stylelint",
    "format": "run-s format:*",
    "format:prettier": "prettier --write --loglevel \"warn\" \"**/*.{js,ts,tsx,json,css,pcss,html,md}\"",
    "format:eslint": "eslint --fix --ext .ts,.tsx",
    "format:stylelint": "stylelint --fix \"src/**/*.{css,pcss}\"",
    "test": "jest ./"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "jest-canvas-mock": "^2.5.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.23.0",
    "i18next": "^23.4.4",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "i18next-locize-backend": "^6.2.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "postcss": "^8.4.26",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.7.1",
    "react-hook-form": "^7.45.2",
    "react-i18next": "^13.1.2",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "ts-jest": "^28.0.8",
